  <select id="getMenuByRoleId" resultMap="menuHashMap"
          parameterType="java.util.Map">
    SELECT
    sm.id,
    sm.href,
    sm.parent_id,
    sm.is_menu,
    sm.name,
    sm.icon_cls,
    sm.menu_type,
    sm.action_tag,
    sm.limit_code,
    sm.sort,
    sm.controller_name
    FROM
    arc_sys_menu sm,arc_sys_role_menu rm
    WHERE sm.id=rm.menu_id
    and sm.is_delete = 0
    and sm.is_menu=#{isMenu}
